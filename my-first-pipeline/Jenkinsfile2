pipeline {
  agent {
    //docker { image 'node' }
    docker { image 'latticeekosz/node10' }
  }
  stages {
    stage('Ubuntu OS Version') {
      steps {
        sh 'uname -a'
      }
    }
    stage('Node version') {
      steps {
        sh 'node -v'
      }
    }
    stage('Testing') {
      steps {
        sh 'node -v'
      }
    }
  }
}
